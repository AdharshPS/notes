  "version": "0.2.0",
  "configurations": [
    {
      "name": "NoteFlow Dev",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "prod",
        "--dart-define=AI_ENABLED=false"
      ]
    },
    {
      "name": "NoteFlow Prod",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "prod",
        "--dart-define=AI_ENABLED=true"
      ]
    },
    {
      "name": "notes (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "notes (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}